<div>
    <table>
        <thead>
            <tr>
                <!--<th>title</th>-->
            </tr>
        </thead>
        <tbody>
            <% @articles.each do |article| %>
            <tr>
                <td>
                    <a href="blog/<%= article.id %>">
                        <%= article.title %>
                    </a>
                    <p><%= article.created_at.strftime("%Y %m %d") %></p>
                    <img src="<%= article.thumbnail %>">
                </td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>